#include<stdio.h>
int main()
{
    int n,temp,r,sum=0;
    printf("enter a number:");
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
        r=n%10;
        sum=sum+r*r*r;
        n=n/10;
    }
    if (sum==temp)
    printf("the given number is armstrong number");
    else
    printf("the given number is not armstrong number");
}
